{
    "name": "Tomorrow Night Eighties",
    "style": "Dark",
    "author": [
        {
            "name": "Chris Kempson",
            "url": "https://github.com/ChrisKempson/Tomorrow-Theme"
        },
        {
            "name": "Christian Tietze"
        }
    ],
    "version": "1.2",

    "editor": {
        "backgroundColor": "#2d2d2d",
        "caretColor": "#cc99cc",
        "selection": {
            "color": "#ffffff",
            "backgroundColor": "#515151",
            "unfocusedBackgroundColor": "#3d3d3d"
        },
        "highlight": {
            "backgroundColor": "#222222",
            "unfocusedBackgroundColor": "#292929"
        }
    },

    "savedSearchesSidebar": {
        "color": "#cccccc",
        "backgroundColor": "#393939",
        "highlightedBackgroundColor": "#515151",
        "border": false,
        "iconColors": {
            "default": "#6699cc"
        }
    },
    
    "resultsList": {
        "color": "#cccccc",
        "backgroundColor": "#2d2d2d",
        "alternateBackgroundColor": "#393939",
        "selection": {
            "color": "#ffffff",
            "backgroundColor": "#515151",
            "unfocusedColor": "#ffffff",
            "unfocusedBackgroundColor": "#515151",
        },
        "border": "#141414",
    },


    "styles": {
        "base": {
            "color": "#cccccc"
        },

        "h1": {
            "font": { "style": "bold" },
            "color": "#66cccc"
        },
        "h2": {
            "font": { "style": "bold" },
            "color": "#66cccc"
        },
        "h3": {
            "font": { "style": "bold" },
            "color": "#66cccc"
        },
        "h4": {
            "font": { "style": "bold" },
            "color": "#66cccc"
        },
        "h5": {
            "font": { "style": "bold" },
            "color": "#66cccc"
        },
        "h6": {
            "font": { "style": "bold" },
            "color": "#66cccc"
        },
        
        "horizontalRule": {
            "color": "#999999"
        },

        "codeBlock": {
            "color": "#999999"
        },
        "inlineCode": {
            "color": "#999999"
        },

        "blockquote": {
            "color": "#f2777a"
        },
        "list": {
            "syntax": { "color" : "#6699cc" }
        },

        "emphasis": {
            "font": { "style": "italic" },
            "color": "#ffcc66"
        },
        "strong": {
            "font": { "style": "bold" },
            "color": "#f99157"
        },

        "image": {
            "color": "#cc99cc"
        },
        "footnote": {
            "color": "#cc99cc"
        },
        "link": {
            "color": "#99cc99"
        },
        "referenceDefinition": {
            "color": "#66cccc"
        },

        "comment": {
            "color": "#888888",
            "font": { "style": "regular" }
        }
    }
}
