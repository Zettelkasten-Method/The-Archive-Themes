{
  "name": "Catppuccin Mocha",
  "style": "Dark",
  "author": [
    {
      "name": "Roger Steve Ruiz",
      "url": "https://rog.gr/",
      "basedOn": "https://github.com/catppuccin/catppuccin"
    }
  ],
  "version": "1.0.0",
  "editor": {
    "backgroundColor": "#1e1e2e",
    "caretColor": "#cba6f7",
    "selection": {
      "color": "#bac2de",
      "unfocusedColor": "#bac2de",
      "backgroundColor": "#313244",
      "unfocusedBackgroundColor": "#313244"
