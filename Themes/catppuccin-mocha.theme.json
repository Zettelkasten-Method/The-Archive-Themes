      "color": "#cdd6f4",
      "syntax": {
        "color": "#cdd6f4"
      }
    },
    "footnote": {
      "color": "#cdd6f4",
      "syntax": {
        "color": "#cdd6f4"
      }
    },
    "link": {
      "color": "#89b4fa",
      "syntax": {
        "color": "#89b4fa"
      }
    },
    "referenceDefinition": {
      "color": "#cdd6f4",
      "syntax": {
        "color": "#cdd6f4"
      }
    },
    "comment": {
      "color": "#6c7086",
      "font": {
        "style": "regular"
      }
    }
