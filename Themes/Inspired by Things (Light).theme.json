{
    "name": "Inspired by Things",
    "style": "Light",
    "author": [
        {
            "name": "Matt Stratford",
            "twitter": "@mattstratford"
        }
    ],
    "version": "0.1",
    "editor": {
        "backgroundColor": "#FFFFFF",
