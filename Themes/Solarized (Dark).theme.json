{
    "name": "Solarized",
    "style": "Dark",
    "author": [
        {
            "name": "Ethan Schoonover"
        },
        {
            "name": "Rudd Fawcett",
            "email": "rudd.fawcett@gmail.com"
        },
        {
            "name": "Christian Tietze"
        }
    ],
    "version": "1.2",

    "editor": {
        "backgroundColor": "#002b36",
        "caretColor": "#839496",
        "highlight": {
            "backgroundColor": "#00161e",
            "unfocusedBackgroundColor": "#00212B"
        },
        "selection": {
            "color": "#002b36",
            "backgroundColor": "#839496",
            "unfocusedBackgroundColor": "#647173"
        }
    },

    "savedSearchesSidebar": {
        "color": "#93a1a1",
        "backgroundColor": "#002b36",
        "highlightedBackgroundColor": "#00161e",
        "border": false,
        "iconColors": {
            "default": "#6c71c4"
        }
    },
    
    "resultsList": {
        "color": "#839496",
        "backgroundColor": "#00212B",
        "alternateBackgroundColor": "#031E20",
        "selection": {
            "color": "#a9bcbb",
            "backgroundColor": "#002b36",
            "unfocusedColor": "#8da0a2",
            "unfocusedBackgroundColor": "#002b36"
        },
        "border": false
    },

    "styles": {
        "base": {
            "color": "#839496"
        },

        "h1": {
            "font": { "style": "bold" },
            "color": "#b58900"
        },
        "h2": {
            "font": { "style": "bold" },
            "color": "#b58900"
        },
        "h3": {
            "font": { "style": "bold" },
            "color": "#b58900"
        },
        "h4": {
            "font": { "style": "bold" },
            "color": "#b58900"
        },
        "h5": {
            "font": { "style": "bold" },
            "color": "#b58900"
        },
        "h6": {
            "font": { "style": "bold" },
            "color": "#b58900"
        },
        
        "horizontalRule": {
            "color": "#b58900"
        },

        "codeBlock": {
            "color": "#b58900",
        },
        "inlineCode": {
            "color": "#b58900",
        },

        "blockquote": {
            "font": { "style": "italic" },
            "indent": { "characters": 2 },
            "color": "#657b83"
        },
        "list": {
            "syntax": { "color" : "#586e75" }
        },

        "emphasis": {
            "color": "#93a1a1",
            "font": { "style": "italic" },
        },
        "strong": {
            "color": "#93a1a1",
            "font": { "style": "bold" },
        },

        "image": {
            "color": "#2aa198"
        },
        "footnote": {
            "color": "#2aa198"
        },
        "link": {
            "color": "#268bd2"
        },
        "referenceDefinition": {
            "color": "#6c71c4"
        },

        "comment": {
            "color": "#586e75",
            "font": { "style": "regular" }
        }
    }
}
