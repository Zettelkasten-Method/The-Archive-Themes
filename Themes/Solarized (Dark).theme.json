{
    "name": "Solarized",
    "style": "Dark",
    "author": [
        {
            "name": "Ethan Schoonover"
        },
        {
            "name": "Rudd Fawcett",
            "email": "rudd.fawcett@gmail.com"
        },
        {
            "name": "Christian Tietze"
        }
    ],
    "version": "1.0",

    "editor": {
        "backgroundColor": "#002b36",
        "caretColor": "#fdf6e3",
        "highlight": {
            "backgroundColor": "#1c6782",
            "unfocusedBackgroundColor": "#2a4e75"
        },
        "selection": {
            "color": "#002b36",
            "backgroundColor": "#d33682",
            "unfocusedBackgroundColor": "#ae3c82"
        }
    },

    "styles": {
        "base": {
            "color": "#839496"
        },

        "h1": {
            "font": { "style": "bold" },
            "color": "#b58900"
        },
        "h2": {
            "font": { "style": "bold" },
            "color": "#b58900"
        },
        "h3": {
            "font": { "style": "bold" },
            "color": "#b58900"
        },
        "h4": {
            "font": { "style": "bold" },
            "color": "#b58900"
        },
        "h5": {
            "font": { "style": "bold" },
            "color": "#b58900"
        },
        "h6": {
            "font": { "style": "bold" },
            "color": "#b58900"
        },
        
        "horizontalRule": {
            "color": "#b58900"
        },

        "codeBlock": {
            "color": "#b58900",
        },
        "inlineCode": {
            "color": "#b58900",
        },

        "blockquote": {
            "font": { "style": "italic" },
            "indent": { "characters": 2 },
            "color": "#839496"
        },
        "list": {
            "syntax": { "color" : "#586e75" }
        },

        "emphasis": {
            "color": "#93a1a1",
            "font": { "style": "italic" },
        },
        "strong": {
            "color": "#93a1a1",
            "font": { "style": "bold" },
        },

        "image": {
            "color": "#cb4b16"
        },
        "footnote": {
            "color": "#cb4b16"
        },
        "link": {
            "color": "#268bd2"
        },
        "referenceDefinition": {
            "color": "#6c71c4"
        },

        "comment": {
            "color": "#586e75",
            "font": { "style": "regular" }
        }
    }
}
